{
  "name": "deg540/kata-de-nota",
  "description": "Kata paranota",
  "type": "project",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.10"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Deg540\\KataDeNota\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deg540\\KataDeNota\\Test\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "test": [
      "phpunit --configuration phpunit.xml"
    ]
  }
}
